{
switch (configuration.dialect()) {
case ASE:
case CUBRID:
case HSQLDB:
case INGRES:
case MYSQL:
case POSTGRES:
case SQLSERVER:
case SYBASE:
    return DSL.exp(argument.mul(two())).sub(one()).div(DSL.exp(argument.mul(two())).add(one()));
default :
  return function("tanh",SQLDataType.NUMERIC,argument);
}
}
