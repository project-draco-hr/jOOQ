{
switch (context.configuration().dialect()) {
case DERBY:
case FIREBIRD:
case SQLITE:
case MARIADB:
case MYSQL:
    return;
}
if (")".equals(parenthesis)) {
  context.formatIndentEnd().formatNewLine();
}
context.sql(parenthesis);
if ("(".equals(parenthesis)) {
  context.formatIndentStart().formatNewLine();
}
}
