{
  jOOQAbstractTest.reset=false;
  WriteListener listener1=new WriteListener();
  B book1=newBook(5);
  B book2=newBook(6);
  create(listener1).batchStore(book1,book2).execute();
  assertEquals(asList("storeStart","storeStart","insertStart","insertStart","insertEnd","insertEnd","storeEnd","storeEnd"),listener1.events);
}
