{
switch (ctx.configuration().dialect()) {
case DERBY:
{
      for (int i=0; i < compareValues.size(); i++) {
        ctx.visit(value);
        ctx.visit(compareValues.get(i));
        ctx.visit(results.get(i));
      }
      break;
    }
default :
{
    ctx.visit(value);
    for (int i=0; i < compareValues.size(); i++) {
      ctx.visit(compareValues.get(i));
      ctx.visit(results.get(i));
    }
    break;
  }
}
if (otherwise != null) {
ctx.visit(otherwise);
}
}
