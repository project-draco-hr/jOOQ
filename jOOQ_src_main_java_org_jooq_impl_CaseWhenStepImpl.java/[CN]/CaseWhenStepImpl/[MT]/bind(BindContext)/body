{
switch (ctx.configuration().getDialect()) {
case DERBY:
{
      for (int i=0; i < compareValues.size(); i++) {
        ctx.bind(value);
        ctx.bind(compareValues.get(i));
        ctx.bind(results.get(i));
      }
      break;
    }
default :
{
    ctx.bind(value);
    for (int i=0; i < compareValues.size(); i++) {
      ctx.bind(compareValues.get(i));
      ctx.bind(results.get(i));
    }
    break;
  }
}
if (otherwise != null) {
ctx.bind(otherwise);
}
}
