{
  sb.append("bind index   [");
  sb.append(index);
  sb.append("]");
  sb.append("\ndeclaring    [");
  if (declareFields) {
    sb.append("fields");
  }
 else   if (declareTables) {
    sb.append("tables");
  }
 else   if (declareWindows) {
    sb.append("windows");
  }
 else {
    sb.append("-");
  }
  sb.append("]\nsubquery     [");
  sb.append(subquery);
  sb.append("]");
}
