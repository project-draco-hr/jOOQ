{
  if (part != null) {
    Clause clause=visitListeners.length > 0 ? clause(part) : null;
    if (clause != null)     start(clause);
    start(part);
    QueryPartInternal internal=(QueryPartInternal)part;
    if (declareFields() && !internal.declaresFields()) {
      declareFields(false);
      visit0(internal);
      declareFields(true);
    }
 else     if (declareTables() && !internal.declaresTables()) {
      declareTables(false);
      visit0(internal);
      declareTables(true);
    }
 else {
      visit0(internal);
    }
    end(part);
    if (clause != null)     end(clause);
  }
  return (C)this;
}
