{
  return create().selectFrom(org.jooq.test.oracle.generatedclasses.tables.TBookStore.T_BOOK_STORE).where(org.jooq.test.oracle.generatedclasses.tables.TBookStore.T_BOOK_STORE.NAME.equal(getValue(org.jooq.test.oracle.generatedclasses.tables.TBookToBookStore.T_BOOK_TO_BOOK_STORE.BOOK_STORE_NAME))).fetchOne();
}
