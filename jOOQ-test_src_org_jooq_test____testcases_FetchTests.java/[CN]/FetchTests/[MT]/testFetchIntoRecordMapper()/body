{
  assertEquals(BOOK_IDS,create().selectFrom(TBook()).orderBy(TBook_ID()).fetch(new RecordMapper<B,Integer>(){
    @Override public Integer map(    B record){
      return record.getValue(TBook_ID());
    }
  }
));
  assertEquals(BOOK_TITLES,create().selectFrom(TBook()).orderBy(TBook_ID()).fetch(new RecordMapper<Record,String>(){
    @Override public String map(    Record record){
      return record.getValue(TBook_TITLE());
    }
  }
));
}
