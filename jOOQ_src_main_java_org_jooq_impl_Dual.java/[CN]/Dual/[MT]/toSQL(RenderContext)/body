{
switch (context.getDialect()) {
case ASE:
case POSTGRES:
case SQLITE:
case SQLSERVER:
    break;
case HSQLDB:
  context.sql("INFORMATION_SCHEMA.SYSTEM_USERS");
break;
case INGRES:
context.sql("(select 1 as dual) as dual");
break;
case DB2:
context.sql("SYSIBM.DUAL");
break;
case DERBY:
context.sql("SYSIBM.SYSDUMMY1");
break;
case SYBASE:
context.sql("SYS.DUMMY");
break;
default :
context.sql("dual");
break;
}
}
