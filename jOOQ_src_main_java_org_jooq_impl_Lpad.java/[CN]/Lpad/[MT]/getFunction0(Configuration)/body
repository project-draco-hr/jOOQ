{
switch (configuration.getDialect()) {
case ASE:
case SQLSERVER:
case SYBASE:
{
      return Factory.concat(Factory.repeat(character,length.sub(Factory.length(field))),field);
    }
case SQLITE:
{
    return Factory.field("replace(replace(substr(quote(zeroblob(({0} + 1) / 2)), 3, ({1} - length({2}))), '''', ''), '0', {3}) || {4}",String.class,length,length,field,character,field);
  }
default :
{
  return function("lpad",SQLDataType.VARCHAR,field,length,character);
}
}
}
