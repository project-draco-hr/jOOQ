{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.person.tables.Contact.CONTACT).where(org.jooq.examples.sqlserver.adventureworks.person.tables.Contact.CONTACT.CONTACTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Employee.EMPLOYEE.CONTACTID))).fetchOne();
}
