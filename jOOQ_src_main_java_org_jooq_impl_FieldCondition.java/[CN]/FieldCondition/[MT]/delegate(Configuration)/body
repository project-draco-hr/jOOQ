{
switch (configuration.dialect().family()) {
case CUBRID:
case FIREBIRD:
case INGRES:
case ORACLE:
case SQLSERVER:
case SYBASE:
    return (QueryPartInternal)condition("{0} = {1}",field,inline(true));
case ASE:
case DB2:
case DERBY:
case H2:
case HSQLDB:
case MARIADB:
case MYSQL:
case POSTGRES:
case SQLITE:
default :
  return (QueryPartInternal)field;
}
}
