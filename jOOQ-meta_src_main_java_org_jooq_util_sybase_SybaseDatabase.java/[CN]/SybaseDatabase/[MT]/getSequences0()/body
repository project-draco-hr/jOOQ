{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  String name : create().select(Syssequence.SEQUENCE_NAME).from(Syssequence.SYSSEQUENCE).orderBy(Syssequence.SEQUENCE_NAME).fetch(Syssequence.SEQUENCE_NAME)) {
    result.add(new DefaultSequenceDefinition(this,name));
  }
  return result;
}
