{
  for (  StackTraceElement e : Thread.currentThread().getStackTrace()) {
    try {
      for (      Method m : Class.forName(e.getClassName()).getMethods()) {
        if (m.getName().equals(e.getMethodName()) && m.getAnnotation(Test.class) != null) {
          return m;
        }
      }
    }
 catch (    Exception ignore) {
    }
  }
  return null;
}
