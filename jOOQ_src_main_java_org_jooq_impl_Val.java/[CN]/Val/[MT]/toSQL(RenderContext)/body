{
switch (context.castMode()) {
case NEVER:
    toSQL(context,getValue(),getType());
  return;
case ALWAYS:
toSQLCast(context);
return;
case SOME:
if (getDataType().isInterval()) {
toSQLCast(context);
}
 else if (context.cast()) {
toSQLCast(context);
}
 else {
toSQL(context,getValue(),getType());
}
return;
}
if (!context.inline()) {
if (!(getValue() instanceof EnumType) && !(getValue() instanceof MasterDataType)) {
switch (context.getDialect()) {
case DB2:
case DERBY:
case H2:
case HSQLDB:
case INGRES:
case POSTGRES:
case SYBASE:
{
toSQLCast(context);
return;
}
}
}
}
if (getDataType().isInterval()) {
toSQLCast(context);
}
 else {
toSQL(context,getValue(),getType());
}
}
