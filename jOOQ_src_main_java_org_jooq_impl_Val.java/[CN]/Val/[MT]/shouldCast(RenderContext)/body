{
  if (!isInline(context)) {
    if (!(value instanceof EnumType)) {
switch (context.getDialect()) {
case DB2:
case DERBY:
case FIREBIRD:
case H2:
case HSQLDB:
case CUBRID:
case POSTGRES:
case SYBASE:
{
          return true;
        }
    }
  }
}
if (getDataType().isInterval()) {
switch (context.getDialect()) {
case ORACLE:
case POSTGRES:
    return true;
}
}
return false;
}
