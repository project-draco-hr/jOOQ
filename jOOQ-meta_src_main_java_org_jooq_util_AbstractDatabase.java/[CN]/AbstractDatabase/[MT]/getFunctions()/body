{
  if (functions == null) {
    functions=new ArrayList<RoutineDefinition>();
    try {
      List<RoutineDefinition> r=getRoutines();
      for (      RoutineDefinition routine : r) {
        if (routine.isSQLUsable()) {
          functions.add(routine);
        }
      }
    }
 catch (    Exception e) {
      log.error("Error while fetching functions",e);
    }
  }
  return functions;
}
