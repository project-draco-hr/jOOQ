{
  if (procedures == null) {
    procedures=new ArrayList<RoutineDefinition>();
    try {
      List<RoutineDefinition> r=getRoutines();
      for (      RoutineDefinition routine : r) {
        if (!routine.isSQLUsable()) {
          procedures.add(routine);
        }
      }
    }
 catch (    Exception e) {
      log.error("Error while fetching procedures",e);
    }
  }
  return procedures;
}
