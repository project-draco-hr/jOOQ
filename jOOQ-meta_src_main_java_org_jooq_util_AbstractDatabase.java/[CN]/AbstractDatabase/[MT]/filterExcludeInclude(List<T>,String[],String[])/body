{
  List<T> result=new ArrayList<T>();
  definitionsLoop:   for (  T definition : definitions) {
    if (excludes != null) {
      for (      String exclude : excludes) {
        if (exclude != null && (definition.getName().matches(exclude.trim()) || definition.getQualifiedName().matches(exclude.trim()))) {
          continue definitionsLoop;
        }
      }
    }
    if (includes != null) {
      for (      String include : includes) {
        if (include != null && (definition.getName().matches(include.trim()) || definition.getQualifiedName().matches(include.trim()))) {
          result.add(definition);
          continue definitionsLoop;
        }
      }
    }
  }
  return result;
}
