{
  assertEquals(0,create().selectCount().from(T_ARRAYS).where(T_ARRAYS.NUMBER_ARRAY.contains((Integer[])null)).fetchOne(0));
  assertEquals(3,create().selectCount().from(T_ARRAYS).where(T_ARRAYS.NUMBER_ARRAY.contains(new Integer[0])).fetchOne(0));
  assertEquals(2,create().selectCount().from(T_ARRAYS).where(T_ARRAYS.NUMBER_ARRAY.contains(new Integer[]{1})).fetchOne(0));
  assertEquals(1,create().selectCount().from(T_ARRAYS).where(T_ARRAYS.NUMBER_ARRAY.contains(new Integer[]{1,2})).fetchOne(0));
  assertEquals(0,create().selectCount().from(T_ARRAYS).where(T_ARRAYS.NUMBER_ARRAY.contains(new Integer[]{1,2,3})).fetchOne(0));
}
