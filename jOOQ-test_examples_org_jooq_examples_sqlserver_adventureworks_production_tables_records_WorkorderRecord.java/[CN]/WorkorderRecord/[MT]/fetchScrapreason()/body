{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Scrapreason.SCRAPREASON).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Scrapreason.SCRAPREASON.SCRAPREASONID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Workorder.WORKORDER.SCRAPREASONID))).fetchOne();
}
