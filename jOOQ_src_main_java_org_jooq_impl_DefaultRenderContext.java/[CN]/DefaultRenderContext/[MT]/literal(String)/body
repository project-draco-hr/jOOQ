{
  if (literal == null) {
    return this;
  }
  if (RenderNameStyle.LOWER == cachedRenderNameStyle) {
    sql(literal.toLowerCase());
  }
 else   if (RenderNameStyle.UPPER == cachedRenderNameStyle) {
    sql(literal.toUpperCase());
  }
 else   if (RenderNameStyle.AS_IS == cachedRenderNameStyle) {
    sql(literal);
  }
 else {
switch (configuration.getDialect()) {
case MYSQL:
      sql("`").sql(literal).sql("`");
    break;
case CUBRID:
case DB2:
case DERBY:
case FIREBIRD:
case H2:
case HSQLDB:
case INGRES:
case ORACLE:
case POSTGRES:
  sql('"').sql(literal).sql('"');
break;
case SQLITE:
sql(literal);
break;
case ASE:
case SQLSERVER:
case SYBASE:
sql("[").sql(literal).sql("]");
break;
default :
sql(literal);
break;
}
}
return this;
}
