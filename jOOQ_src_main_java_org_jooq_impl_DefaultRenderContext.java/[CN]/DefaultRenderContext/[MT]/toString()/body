{
  StringBuilder sb=new StringBuilder();
  sb.append("rendering [");
  sb.append(render());
  sb.append("]");
  sb.append("\ninlining  [");
  sb.append(inline);
  sb.append("]\ndeclaring [");
  if (declareFields) {
    sb.append("fields");
  }
 else   if (declareTables) {
    sb.append("tables");
  }
 else {
    sb.append("-");
  }
  sb.append("]");
  return sb.toString();
}
