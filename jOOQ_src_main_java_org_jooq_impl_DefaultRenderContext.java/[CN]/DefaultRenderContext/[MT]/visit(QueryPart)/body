{
  if (part != null) {
    Clause[] clauses=visitListeners.length > 0 ? clause(part) : null;
    if (clauses != null)     for (int i=0; i < clauses.length; i++)     start(clauses[i]);
    start(part);
    super.visit(part);
    end(part);
    if (clauses != null)     for (int i=clauses.length - 1; i >= 0; i--)     end(clauses[i]);
  }
  return this;
}
