{
  if (paramType == INLINED)   return;
  if (part instanceof Param) {
    if (((Param<?>)part).isInline())     return;
switch (configuration().dialect()) {
case ASE:
      checkForceInline(2000);
    return;
case INGRES:
  checkForceInline(1024);
return;
case SQLITE:
checkForceInline(999);
return;
case SQLSERVER:
checkForceInline(2100);
return;
default :
return;
}
}
}
