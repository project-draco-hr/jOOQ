{
  this.fields=f;
  this.mainKey=new boolean[f.length];
  if (updatable != null) {
    for (int i=0; i < fields.length; i++) {
      if (fields[i] != null) {
        if (updatable.getMainKey().getFields().contains(fields[i])) {
          mainKey[i]=true;
        }
      }
    }
  }
  return this;
}
