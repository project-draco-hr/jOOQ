{
  StringWriter writer=new StringWriter();
  JAXB.marshal(settings,writer);
  return "DefaultConfiguration [\n\tconnected=" + (connectionProvider != null && !(connectionProvider instanceof NoConnectionProvider)) + ",\n\tdialect="+ dialect+ ",\n\tdata="+ data+ ",\n\tsettings=\n\t\t"+ writer.toString().trim().replace("\n","\n\t\t")+ "\n]";
}
