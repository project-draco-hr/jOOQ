{
  A author=create().selectFrom(TAuthor()).where(TAuthor_FIRST_NAME().equalIgnoreCase(TAuthor_FIRST_NAME())).and(upper(TAuthor_FIRST_NAME()).equalIgnoreCase(lower(TAuthor_FIRST_NAME()))).and(TAuthor_FIRST_NAME().equalIgnoreCase("george")).and(TAuthor_FIRST_NAME().equalIgnoreCase("geORge")).and(TAuthor_FIRST_NAME().equalIgnoreCase("GEORGE")).and(TAuthor_FIRST_NAME().notEqualIgnoreCase("paulo")).fetchOne();
  assertEquals("George",author.getValue(TAuthor_FIRST_NAME()));
}
