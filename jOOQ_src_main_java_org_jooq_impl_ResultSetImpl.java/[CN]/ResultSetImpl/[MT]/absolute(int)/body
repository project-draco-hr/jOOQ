{
  if (result.size() > 0) {
    if (row > 0) {
      if (row <= result.size()) {
        index=row;
        return true;
      }
 else {
        afterLast();
        return false;
      }
    }
 else     if (row == 0) {
      beforeFirst();
      return false;
    }
 else {
      if (-row <= result.size()) {
        index=result.size() + 1 + row;
        return true;
      }
 else {
        beforeFirst();
        return false;
      }
    }
  }
 else {
    return false;
  }
}
