{
  jOOQAbstractTest.reset=false;
  assertEquals(1,create().insertInto(tableByName(TAuthor().getName()),fieldByName(TAuthor().getName(),TAuthor_ID().getName()),fieldByName(TAuthor().getName(),TAuthor_LAST_NAME().getName())).values(3,"X").execute());
