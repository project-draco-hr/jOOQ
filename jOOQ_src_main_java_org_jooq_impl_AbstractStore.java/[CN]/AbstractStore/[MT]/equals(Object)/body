{
  if (obj instanceof Store) {
    final Store<?> that=(Store<?>)obj;
    if (size() == that.size()) {
      for (int i=0; i < size(); i++) {
        final Object thisValue=getValue(i);
        final Object thatValue=that.getValue(i);
        if (thisValue == null) {
          if (thatValue != null) {
            return false;
          }
        }
 else         if (!thisValue.equals(thatValue)) {
          return false;
        }
      }
      return true;
    }
  }
  return false;
}
