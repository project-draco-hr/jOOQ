{
  if (values != null) {
    Object[] result=new Object[values.length];
    for (int i=0; i < values.length; i++) {
      if (values[i] instanceof Field<?>) {
        result[i]=values[i];
      }
 else {
        result[i]=convert(values[i],fields[i].getType());
      }
    }
    return result;
  }
 else {
    return null;
  }
}
