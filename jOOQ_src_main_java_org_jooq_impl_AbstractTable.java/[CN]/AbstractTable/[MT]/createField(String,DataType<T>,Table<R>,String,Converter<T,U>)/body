{
  final DataType<U> actualType=converter == null ? (DataType<U>)type : type.asConvertedDataType(converter);
  final TableFieldImpl<R,U> tableField=new TableFieldImpl<R,U>(name,actualType,table,comment,converter);
  if (table instanceof TableImpl) {
    ((TableImpl<?>)table).fields0().add(tableField);
  }
  return tableField;
}
