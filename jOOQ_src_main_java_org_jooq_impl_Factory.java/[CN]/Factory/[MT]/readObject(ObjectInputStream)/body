{
  in.defaultReadObject();
  if (log.isTraceEnabled()) {
    log.trace("Deserialising",this);
  }
  Configuration registered=org.jooq.ConfigurationRegistry.provideFor(this);
  if (registered != null) {
    connection=registered.getConnection();
  }
