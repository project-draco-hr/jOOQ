{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesorderheader.SALESORDERHEADER).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesorderheader.SALESORDERHEADER.CONTACTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.person.tables.Contact.CONTACT.CONTACTID))).fetch();
