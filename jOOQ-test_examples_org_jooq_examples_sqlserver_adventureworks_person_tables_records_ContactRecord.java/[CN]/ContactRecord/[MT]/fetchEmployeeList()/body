{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Employee.EMPLOYEE).where(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Employee.EMPLOYEE.CONTACTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.person.tables.Contact.CONTACT.CONTACTID))).fetch();
}
