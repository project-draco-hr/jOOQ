{
switch (configuration.dialect().family()) {
case ASE:
case INGRES:
case SQLSERVER:
case SYBASE:
case CUBRID:
case HSQLDB:
case MARIADB:
case MYSQL:
case POSTGRES:
    return DSL.exp(argument.mul(two())).add(one()).div(DSL.exp(argument).mul(two()));
default :
  return function("cosh",SQLDataType.NUMERIC,argument);
}
}
