{
switch (configuration.getDialect()) {
case ASE:
case CUBRID:
case HSQLDB:
case INGRES:
case MYSQL:
case POSTGRES:
case SQLSERVER:
case SYBASE:
    return DSL.exp(argument.mul(two())).add(one()).div(DSL.exp(argument).mul(two()));
default :
  return function("cosh",SQLDataType.NUMERIC,argument);
}
}
