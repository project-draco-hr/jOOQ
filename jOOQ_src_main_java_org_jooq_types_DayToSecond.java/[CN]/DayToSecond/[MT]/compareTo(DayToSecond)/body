{
  if (days < that.days) {
    return -1;
  }
  if (days > that.days) {
    return 1;
  }
  if (hours < that.hours) {
    return -1;
  }
  if (hours > that.hours) {
    return 1;
  }
  if (minutes < that.minutes) {
    return -1;
  }
  if (minutes > that.minutes) {
    return 1;
  }
  if (seconds < that.seconds) {
    return -1;
  }
  if (seconds > that.seconds) {
    return 1;
  }
  if (nano < that.nano) {
    return -1;
  }
  if (nano > that.nano) {
    return 1;
  }
  return 0;
}
