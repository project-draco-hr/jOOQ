{
  assertEquals(asList(1,2),create().selectDistinct(TBookToBookStore_BOOK_ID()).from(TBookToBookStore()).orderBy(TBookToBookStore_BOOK_ID()).limit(2).fetch(TBookToBookStore_BOOK_ID()));
