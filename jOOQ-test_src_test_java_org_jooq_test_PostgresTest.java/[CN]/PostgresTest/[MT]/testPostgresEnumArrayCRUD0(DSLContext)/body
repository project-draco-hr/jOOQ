{
  jOOQAbstractTest.reset=false;
  TArrays a=T_ARRAYS;
  assertEquals(4,create.insertInto(a,a.ID,a.ENUM_ARRAY).values(11,null).values(12,new UCountry[0]).values(13,new UCountry[]{null}).values(14,new UCountry[]{UCountry.Brazil}).execute());
  List<UCountry[]> countries=create.select(a.ENUM_ARRAY).from(a).where(a.ID.gt(10)).orderBy(a.ID).fetch(a.ENUM_ARRAY);
  assertNull(countries.get(0));
  assertEquals(0,countries.get(1).length);
  assertEquals(1,countries.get(2).length);
  assertNull(countries.get(2)[0]);
  assertEquals(1,countries.get(3).length);
  assertEquals(UCountry.Brazil,countries.get(3)[0]);
}
