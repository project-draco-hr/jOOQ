{
  if (context.subquery()) {
    context.keyword(operator.toSQL()).sql(" (").formatIndentStart().formatNewLine().visit(query).formatIndentEnd().formatNewLine().sql(")");
  }
 else {
    context.keyword(operator.toSQL()).sql(" (").subquery(true).formatIndentStart().formatNewLine().visit(query).formatIndentEnd().formatNewLine().subquery(false).sql(")");
  }
}
