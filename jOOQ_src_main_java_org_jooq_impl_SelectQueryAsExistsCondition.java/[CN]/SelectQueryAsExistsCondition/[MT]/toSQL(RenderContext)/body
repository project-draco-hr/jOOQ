{
  if (context.subquery()) {
    context.keyword(operator.toSQL()).sql(" (").formatIndentLockStart().sql(query).formatIndentLockEnd().sql(")");
  }
 else {
    context.keyword(operator.toSQL()).sql(" (").subquery(true).formatIndentLockStart().sql(query).formatIndentLockEnd().subquery(false).sql(")");
  }
}
