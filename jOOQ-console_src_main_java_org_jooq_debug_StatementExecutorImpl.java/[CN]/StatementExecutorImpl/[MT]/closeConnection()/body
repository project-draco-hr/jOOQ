{
  if (conn != null) {
    if (stmt != null) {
      try {
        stmt.cancel();
      }
 catch (      Exception e) {
      }
      try {
        stmt.close();
      }
 catch (      Exception e) {
      }
    }
    stmt=null;
    try {
      conn.close();
    }
 catch (    Exception e) {
    }
    conn=null;
  }
}
