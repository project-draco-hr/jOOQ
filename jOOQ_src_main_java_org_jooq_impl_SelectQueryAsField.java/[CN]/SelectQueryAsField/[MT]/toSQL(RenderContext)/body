{
  if (context.subquery()) {
    context.sql("(").formatIndentStart().formatNewLine().sql(query).formatIndentEnd().formatNewLine().sql(")");
  }
 else {
    context.sql("(").subquery(true).formatIndentStart().formatNewLine().sql(query).formatIndentEnd().formatNewLine().subquery(false).sql(")");
  }
}
