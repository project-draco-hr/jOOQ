{
switch (context.getDialect()) {
case ORACLE:
{
      if (context.inline()) {
        toSQLInline(context);
      }
 else {
        context.sql("?");
      }
      return;
    }
case POSTGRES:
{
    toSQLInline(context);
    return;
  }
case DB2:
{
  context.sql(getInlineConstructor(context));
  context.sql("()");
  String separator="..";
  for (  Field<?> field : value.fields()) {
    context.sql(separator);
    context.sql(field.getName());
    context.sql("(");
    context.sql(val(value.getValue(field)));
    context.sql(")");
  }
  return;
}
default :
toSQLInline(context);
return;
}
}
