{
switch (context.getDialect()) {
case POSTGRES:
    return "ROW";
case ORACLE:
case DB2:
default :
{
    UDT<?> udt=record.getUDT();
    if (Util.getMappedSchema(context,udt.getSchema()) != null) {
      return Util.getMappedSchema(context,udt.getSchema()) + "." + udt.getName();
    }
 else {
      return udt.getName();
    }
  }
}
}
