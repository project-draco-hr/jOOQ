{
switch (context.getDialect()) {
case POSTGRES:
    return "ROW";
case ORACLE:
case DB2:
default :
{
    UDT<?> udt=record.getUDT();
    Schema mappedSchema=Util.getMappedSchema(context,udt.getSchema());
    if (mappedSchema != null) {
      return mappedSchema + "." + udt.getName();
    }
 else {
      return udt.getName();
    }
  }
}
}
