{
  context.sql(getInlineConstructor(context));
  context.sql("(");
  String separator="";
  for (  Field<?> field : record.getFields()) {
    Field<?> value=create(context).val(record.getValue(field));
    context.sql(separator);
    context.sql(value);
    separator=", ";
  }
  context.sql(")");
}
