{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesperson.SALESPERSON).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesperson.SALESPERSON.TERRITORYID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesterritory.SALESTERRITORY.TERRITORYID))).fetch();
}
