{
  Class<? extends T> type=field.getType();
  Class<?> actual=type;
  Converter<?,? extends T> converter=DataTypes.converter(type);
  if (converter != null) {
    actual=converter.fromType();
  }
  Object result=getFromResultSet(ctx,actual,index);
  if (converter != null) {
    result=((Converter)converter).from(result);
  }
  return (T)result;
}
