{
  Result<TAuthorRecord> a1=create().selectFrom(T_AUTHOR.versionsBetweenScnMinvalue().andMaxvalue()).orderBy(T_AUTHOR.ID).fetch();
  assertTrue(a1.size() >= 2);
  create().select(max(versionsStartscn()),max(versionsStarttime()),max(versionsEndscn()),max(versionsEndtime()),max(versionsXid()),max(versionsOperation())).from(T_AUTHOR.versionsBetweenTimestampMinvalue().andMaxvalue()).fetch();
}
