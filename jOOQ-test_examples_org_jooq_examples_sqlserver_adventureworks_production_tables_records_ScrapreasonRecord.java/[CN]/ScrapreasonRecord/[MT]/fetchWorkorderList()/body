{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Workorder.WORKORDER).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Workorder.WORKORDER.SCRAPREASONID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Scrapreason.SCRAPREASON.SCRAPREASONID))).fetch();
}
