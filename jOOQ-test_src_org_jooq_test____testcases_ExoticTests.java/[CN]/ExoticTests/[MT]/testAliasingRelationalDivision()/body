{
  Record record=create().select().from(TBookToBookStore().divideBy(TBookStore()).on(TBookToBookStore_BOOK_STORE_NAME().equal(TBookStore_NAME())).returning(TBookToBookStore_BOOK_ID(),TBookToBookStore_BOOK_ID()).as("division","x","y")).fetchOne();
  assertEquals(3,record.getValue("x"));
  assertEquals(3,record.getValue("y"));
}
