{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  Record record : create().select(Sequences.SEQUENCE_NAME,Sequences.DATA_TYPE).from(SEQUENCES).where(Sequences.SEQUENCE_SCHEMA.equal(getSchemaName())).orderBy(Sequences.SEQUENCE_NAME).fetch()) {
    DataTypeDefinition type=new DefaultDataTypeDefinition(this,record.getValue(Sequences.DATA_TYPE),0,0);
    result.add(new DefaultSequenceDefinition(getSchema(),record.getValue(Sequences.SEQUENCE_NAME),type));
  }
  return result;
}
