{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  Record record : create().select(SEQUENCES.SEQUENCE_NAME,SEQUENCES.DATA_TYPE,SEQUENCES.NUMERIC_PRECISION,SEQUENCES.NUMERIC_SCALE).from(SEQUENCES).where(SEQUENCES.SEQUENCE_SCHEMA.equal(getSchemaName())).orderBy(SEQUENCES.SEQUENCE_NAME).fetch()) {
    DataTypeDefinition type=new DefaultDataTypeDefinition(this,record.getValue(SEQUENCES.DATA_TYPE),record.getValue(SEQUENCES.NUMERIC_PRECISION),record.getValue(SEQUENCES.NUMERIC_SCALE));
    result.add(new DefaultSequenceDefinition(getSchema(),record.getValue(SEQUENCES.SEQUENCE_NAME),type));
  }
  return result;
}
