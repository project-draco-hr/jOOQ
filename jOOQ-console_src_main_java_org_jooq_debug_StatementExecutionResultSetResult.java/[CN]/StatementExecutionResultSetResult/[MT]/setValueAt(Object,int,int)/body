{
  if (Utils.equals(o,rowData[row][col])) {
    return false;
  }
  try {
    rs.absolute(row + 1);
    rs.updateObject(col + 1,o);
    rs.updateRow();
    rowData[row][col]=o;
    return true;
  }
 catch (  SQLException e) {
    e.printStackTrace();
    try {
      rs.cancelRowUpdates();
    }
 catch (    SQLException ex) {
      ex.printStackTrace();
    }
  }
  return false;
}
