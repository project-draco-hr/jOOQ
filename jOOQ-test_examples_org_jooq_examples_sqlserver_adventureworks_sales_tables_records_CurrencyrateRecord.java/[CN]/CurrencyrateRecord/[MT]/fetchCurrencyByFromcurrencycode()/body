{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currency.CURRENCY).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currency.CURRENCY.CURRENCYCODE.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currencyrate.CURRENCYRATE.FROMCURRENCYCODE))).fetchOne();
}
