{
switch (configuration.getDialect()) {
case ORACLE:
case DB2:
    if (arguments.length == 0) {
      return "";
    }
 else {
      return delimiter;
    }
case SQLITE:
case DERBY:
case H2:
case HSQLDB:
case INGRES:
case MYSQL:
case POSTGRES:
case SQLSERVER:
case SYBASE:
default :
  return delimiter;
}
}
