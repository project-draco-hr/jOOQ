{
  toSQLFunctionName(context);
  context.sql("(");
  if (distinct) {
    context.keyword("distinct").sql(" ");
  }
  if (!arguments.isEmpty()) {
    context.visit(arguments);
  }
  if (ignoreNulls) {
{
      context.sql(" ").keyword("ignore nulls");
    }
  }
 else   if (respectNulls) {
{
      context.sql(" ").keyword("respect nulls");
    }
  }
  context.sql(")");
}
