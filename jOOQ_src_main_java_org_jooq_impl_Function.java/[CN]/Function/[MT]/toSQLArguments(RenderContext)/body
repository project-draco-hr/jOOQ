{
  toSQLFunctionName(ctx);
  ctx.sql("(");
  if (distinct) {
    ctx.keyword("distinct").sql(" ");
  }
  if (!arguments.isEmpty()) {
    ctx.visit(arguments);
  }
  if (ignoreNulls) {
{
      ctx.sql(" ").keyword("ignore nulls");
    }
  }
 else   if (respectNulls) {
{
      ctx.sql(" ").keyword("respect nulls");
    }
  }
  ctx.sql(")");
}
