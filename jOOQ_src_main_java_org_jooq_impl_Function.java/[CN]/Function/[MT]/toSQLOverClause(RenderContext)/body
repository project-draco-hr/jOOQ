{
  if (windowSpecification == null && windowDefinition == null && windowName == null)   return;
  if (term == ROW_NUMBER && ctx.configuration().dialect() == HSQLDB)   return;
  ctx.sql(" ").keyword("over").sql(" (").visit(windowSpecification != null ? windowSpecification : windowDefinition != null ? windowDefinition : windowName).sql(")");
}
