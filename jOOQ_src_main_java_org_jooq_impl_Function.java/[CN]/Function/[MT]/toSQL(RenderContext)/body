{
  context.sql(getFNPrefix());
  context.sql(getName());
  context.sql(getArgumentListDelimiter(context,"("));
  String separator="";
  for (  NamedQueryPart field : arguments) {
    context.sql(separator);
    toSQLField(context,field);
    separator=", ";
  }
  context.sql(getArgumentListDelimiter(context,")"));
  context.sql(getFNSuffix());
}
