{
  toSQLFunctionName(context);
  context.sql("(");
  if (distinct) {
    context.keyword("distinct").sql(" ");
  }
  context.visit(arguments.get(0));
  if (!withinGroupOrderBy.isEmpty()) {
    context.sql(" ").keyword("order by").sql(" ").visit(withinGroupOrderBy);
  }
  if (arguments.size() > 1) {
    context.sql(" ").keyword("separator").sql(" ").visit(arguments.get(1));
  }
  context.sql(")");
}
