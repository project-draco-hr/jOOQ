{
  context.sql(getFNName(context.configuration().getDialect()));
  context.sql("(");
  if (distinct) {
    context.keyword("distinct ");
  }
  context.sql(arguments.get(0));
  if (!withinGroupOrderBy.isEmpty()) {
    context.keyword(" order by ").sql(withinGroupOrderBy);
  }
  if (arguments.size() > 1) {
    context.keyword(" separator ").sql(arguments.get(1));
  }
  context.sql(")");
}
