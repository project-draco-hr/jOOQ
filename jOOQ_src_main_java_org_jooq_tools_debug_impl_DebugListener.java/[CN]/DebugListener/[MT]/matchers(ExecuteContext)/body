{
  if (matchers == null) {
    matchers=new ArrayList<Matcher>();
    for (    Debugger debugger : LocalRegistry.get()) {
      for (      Matcher matcher : debugger.matchers()) {
        if (((MatcherImpl)matcher).matches(ctx)) {
          matchers.add(matcher);
        }
      }
    }
  }
  return matchers;
}
