{
  iterator();
  ResultImpl<R> result=new ResultImpl<R>(ctx.configuration(),fields);
  ctx.result(result);
  listener.resultStart(ctx);
  for (int i=0; i < number && iterator().hasNext(); i++) {
    result.addRecord(iterator().next());
  }
  ctx.result(result);
  listener.resultEnd(ctx);
  return result;
}
