{
  iterator();
  ResultImpl<R> result=new ResultImpl<R>(ctx.configuration(),fields);
  R record=null;
  ctx.result(result);
  listener.resultStart(ctx);
  for (int i=0; i < number && ((record=iterator().next()) != null); i++) {
    result.addRecord(record);
  }
  ctx.result(result);
  listener.resultEnd(ctx);
  return result;
}
