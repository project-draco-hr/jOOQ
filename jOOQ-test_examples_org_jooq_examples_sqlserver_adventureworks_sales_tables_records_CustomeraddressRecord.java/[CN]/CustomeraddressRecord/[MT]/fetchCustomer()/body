{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Customer.CUSTOMER).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Customer.CUSTOMER.CUSTOMERID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Customeraddress.CUSTOMERADDRESS.CUSTOMERID))).fetchOne();
}
