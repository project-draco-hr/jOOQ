{
  if (alias != null) {
    alias.toSQL(context);
  }
 else {
    if (context.qualify()) {
      Schema mappedSchema=Utils.getMappedSchema(context.configuration(),getSchema());
      if (mappedSchema != null) {
        context.visit(mappedSchema);
        context.sql(".");
      }
    }
    context.literal(Utils.getMappedTable(context.configuration(),this).getName());
  }
}
