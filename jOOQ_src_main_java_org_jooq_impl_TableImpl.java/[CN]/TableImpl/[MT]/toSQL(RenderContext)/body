{
  if (alias != null) {
    alias.toSQL(context);
  }
 else {
    if (context.qualify()) {
      Schema mappedSchema=Util.getMappedSchema(context,getSchema());
      if (mappedSchema != null) {
        context.sql(mappedSchema);
        context.sql(".");
      }
    }
    context.literal(Util.getMappedTable(context,this).getName());
  }
}
