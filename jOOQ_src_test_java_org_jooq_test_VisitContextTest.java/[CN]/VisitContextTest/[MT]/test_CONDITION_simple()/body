{
  ctx.render(FIELD_ID1.eq(1));
  assertEvents(asList(asList(CONDITION),asList(CONDITION,CONDITION_COMPARISON),asList(CONDITION,CONDITION_COMPARISON,FIELD),asList(CONDITION,CONDITION_COMPARISON,FIELD,FIELD_REFERENCE),asList(CONDITION,CONDITION_COMPARISON,FIELD),asList(CONDITION,CONDITION_COMPARISON,FIELD,FIELD_VALUE)));
}
