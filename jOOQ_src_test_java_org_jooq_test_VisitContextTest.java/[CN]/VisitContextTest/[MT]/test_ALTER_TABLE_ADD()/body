{
  assertEvents(asList(asList(ALTER_TABLE),asList(ALTER_TABLE,ALTER_TABLE_TABLE),asList(ALTER_TABLE,ALTER_TABLE_TABLE,TABLE),asList(ALTER_TABLE,ALTER_TABLE_TABLE,TABLE,TABLE_REFERENCE),asList(ALTER_TABLE,ALTER_TABLE_ADD),asList(ALTER_TABLE,ALTER_TABLE_ADD,FIELD),asList(ALTER_TABLE,ALTER_TABLE_ADD,FIELD,FIELD_REFERENCE)),ctx.alterTable(TABLE1).add(FIELD_NAME1,SQLDataType.VARCHAR));
