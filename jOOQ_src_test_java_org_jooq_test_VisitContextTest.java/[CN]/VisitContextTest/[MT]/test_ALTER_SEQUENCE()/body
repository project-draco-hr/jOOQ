{
  ctx.configuration().set(POSTGRES);
  assertEvents(asList(asList(ALTER_SEQUENCE),asList(ALTER_SEQUENCE,ALTER_SEQUENCE_SEQUENCE),asList(ALTER_SEQUENCE,ALTER_SEQUENCE_SEQUENCE,SEQUENCE),asList(ALTER_SEQUENCE,ALTER_SEQUENCE_SEQUENCE,SEQUENCE,SEQUENCE_REFERENCE),asList(ALTER_SEQUENCE,ALTER_SEQUENCE_RESTART)),ctx.alterSequence("seq").restart());
}
