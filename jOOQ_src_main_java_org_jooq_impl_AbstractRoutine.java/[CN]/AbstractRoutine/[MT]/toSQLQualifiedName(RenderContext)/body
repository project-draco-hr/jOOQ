{
  Schema mappedSchema=Util.getMappedSchema(context,getSchema());
  if (context.qualify()) {
    if (mappedSchema != null) {
      context.sql(mappedSchema);
      context.sql(".");
    }
    if (getPackage() != null) {
      context.sql(getPackage());
      context.sql(".");
    }
  }
  context.literal(getName());
}
