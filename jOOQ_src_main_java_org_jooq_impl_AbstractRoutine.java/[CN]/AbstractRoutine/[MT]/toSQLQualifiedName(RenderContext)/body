{
  Schema mappedSchema=Utils.getMappedSchema(context.configuration(),getSchema());
  if (context.qualify()) {
    if (mappedSchema != null) {
      context.visit(mappedSchema);
      context.sql('.');
    }
    if (getPackage() != null) {
      context.visit(DSL.name(getPackage().getName()));
      context.sql('.');
    }
  }
  context.literal(getName());
}
