{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Productinventory.PRODUCTINVENTORY).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Productinventory.PRODUCTINVENTORY.LOCATIONID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Location.LOCATION.LOCATIONID))).fetch();
}
