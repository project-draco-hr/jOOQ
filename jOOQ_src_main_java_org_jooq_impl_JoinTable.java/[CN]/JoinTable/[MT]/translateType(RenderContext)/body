{
  if (simulateCrossJoin(context)) {
    return JOIN;
  }
 else   if (simulateNaturalJoin(context)) {
    return JOIN;
  }
 else   if (simulateNaturalLeftOuterJoin(context)) {
    return LEFT_OUTER_JOIN;
  }
 else   if (simulateNaturalRightOuterJoin(context)) {
    return RIGHT_OUTER_JOIN;
  }
 else {
    return type;
  }
}
