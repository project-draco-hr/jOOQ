{
  context.visit(lhs).formatIndentStart().formatSeparator().keyword(translateType(context).toSQL()).sql(" ");
  if (rhs instanceof JoinTable) {
    context.sql("(").formatIndentStart().formatNewLine();
  }
  context.visit(rhs);
  if (rhs instanceof JoinTable) {
    context.formatIndentEnd().formatNewLine().sql(")");
  }
  if (!rhsPartitionBy.isEmpty()) {
    context.formatSeparator().keyword("partition by (").visit(rhsPartitionBy).sql(")");
  }
  if (!asList(CROSS_JOIN,NATURAL_JOIN,NATURAL_LEFT_OUTER_JOIN,NATURAL_RIGHT_OUTER_JOIN).contains(translateType(context))) {
    toSQLJoinCondition(context);
  }
  context.formatIndentEnd();
}
