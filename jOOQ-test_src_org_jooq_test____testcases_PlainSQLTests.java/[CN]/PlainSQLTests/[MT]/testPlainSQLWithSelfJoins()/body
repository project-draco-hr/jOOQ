{
  Result<Record> result1=create().fetch("select * from (select id from t_author) a1, (select id from t_author) a2 order by a1.id, a2.id");
  assertEquals(asList(1,1,2,2),result1.getValues(0,int.class));
  assertEquals(asList(1,2,1,2),result1.getValues(1,int.class));
  Result<Record> result2=create().fetch("select * from (select id from t_author) a1 left outer join (select id from t_author where 1 = 0) a2 on a1.id = a2.id");
  assertEquals(asList(1,2),result2.getValues(0,Integer.class));
  assertEquals(asList(null,null),result2.getValues(1,Integer.class));
}
