{
  Result<Record> result=create().select(fieldByName(Integer.class,TBook_ID().getName()),fieldByName(String.class,TBook_TITLE().getName())).from(tableByName(TBook().getName())).orderBy(fieldByName(TBook().getName(),TBook_ID().getName())).fetch();
  assertEquals(4,result.size());
  assertEquals(BOOK_IDS,result.getValues(0));
  assertEquals(BOOK_TITLES,result.getValues(1));
}
