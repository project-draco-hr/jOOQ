{
  if (text == null) {
    return false;
  }
switch (type) {
case STARTS_WITH:
{
      for (      String sql : texts) {
        if (isCaseSensitive) {
          if (sql.startsWith(text)) {
            return true;
          }
        }
 else         if (sql.length() >= text.length()) {
          if (sql.substring(0,text.length()).equalsIgnoreCase(text)) {
            return true;
          }
        }
      }
      return false;
    }
case EQUALS:
{
    for (    String sql : texts) {
      if (isCaseSensitive) {
        if (sql.equals(text)) {
          return true;
        }
      }
 else {
        if (sql.equalsIgnoreCase(text)) {
          return true;
        }
      }
    }
    return false;
  }
case CONTAINS:
{
  for (  String sql : texts) {
    if (pattern.matcher(sql).matches()) {
      return true;
    }
  }
  return false;
}
case MATCHES_REG_EXP:
{
for (String sql : texts) {
  if (pattern.matcher(sql).matches()) {
    return true;
  }
}
return false;
}
}
return false;
}
