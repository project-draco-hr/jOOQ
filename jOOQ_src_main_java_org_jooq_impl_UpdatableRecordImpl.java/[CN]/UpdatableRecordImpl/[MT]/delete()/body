{
  try {
    DeleteQuery<R> delete=create().deleteQuery(getTable());
    for (    Field<?> field : getMainKey().getFields()) {
      addCondition(delete,field);
    }
    return delete.execute();
  }
  finally {
    for (    Field<?> field : getFields()) {
      getValue0(field).setChanged(true);
    }
  }
}
