{
  return create().selectFrom(org.jooq.test.ase.generatedclasses.tables.XUnused.X_UNUSED).where(org.jooq.test.ase.generatedclasses.tables.XUnused.ID.equal(getValue(org.jooq.test.ase.generatedclasses.tables.XUnused.ID_REF))).and(org.jooq.test.ase.generatedclasses.tables.XUnused.NAME.equal(getValue(org.jooq.test.ase.generatedclasses.tables.XUnused.NAME_REF))).fetchOne();
}
