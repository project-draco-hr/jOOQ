{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Document.DOCUMENT).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Document.DOCUMENT.DOCUMENTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Productdocument.PRODUCTDOCUMENT.DOCUMENTID))).fetchOne();
