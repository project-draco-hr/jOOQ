{
  String s=super.toString();
  if (args == null || args.length == 0) {
    return s + "()";
  }
  StringBuilder sb=new StringBuilder();
  sb.append(s).append('(');
  for (int i=0; i < args.length; i++) {
    Object arg=args[i];
    if (i > 0) {
      sb.append(", ");
    }
    if (arg != null && arg.getClass().isArray()) {
      sb.append(Arrays.deepToString((Object[])arg));
    }
 else {
      sb.append(arg);
    }
  }
  sb.append(')');
  return sb.toString();
}
