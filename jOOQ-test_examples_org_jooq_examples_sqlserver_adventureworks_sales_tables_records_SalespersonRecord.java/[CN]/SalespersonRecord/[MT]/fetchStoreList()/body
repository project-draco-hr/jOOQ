{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Store.STORE).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Store.STORE.SALESPERSONID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesperson.SALESPERSON.SALESPERSONID))).fetch();
