{
  List<TableDefinition> result=new ArrayList<TableDefinition>();
  for (  String name : create().select(TABLES.TABLE_NAME).from(TABLES).where(TABLES.TABLE_SCHEMA.equal(getSchemaName())).orderBy(TABLES.TABLE_NAME).fetch(TABLES.TABLE_NAME)) {
    String comment="";
    result.add(new SQLServerTableDefinition(this,name,comment));
  }
  return result;
}
