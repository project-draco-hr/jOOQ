{
switch (ctx.family()) {
case FIREBIRD:
{
      ctx.keyword("execute block").formatSeparator().keyword("as").formatSeparator().keyword("begin").formatIndentStart().formatSeparator().keyword("execute statement").sql(" '").stringLiteral(true).formatIndentStart().formatSeparator();
      break;
    }
default :
  break;
}
}
