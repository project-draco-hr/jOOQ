{
  if (key != null && !key.isEmpty()) {
    R record=query.getReturnedRecord();
    if (record != null) {
      for (      Field<?> field : key) {
        int index=indexOrFail(fieldsRow(),field);
        Object value=record.get(field);
        values[index]=value;
        originals[index]=value;
      }
    }
    if (asList(DERBY,H2,MARIADB,MYSQL).contains(configuration().family()) && this instanceof UpdatableRecord)     ((UpdatableRecord<?>)this).refresh(key.toArray(new Field[0]));
  }
}
