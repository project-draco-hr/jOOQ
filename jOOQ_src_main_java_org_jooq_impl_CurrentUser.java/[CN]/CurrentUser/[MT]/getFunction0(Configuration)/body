{
switch (configuration.dialect().family()) {
case ASE:
case ORACLE:
    return field("user",SQLDataType.VARCHAR);
case DB2:
case INGRES:
case SQLSERVER:
case SYBASE:
case DERBY:
case FIREBIRD:
case HSQLDB:
case POSTGRES:
case SQLITE:
  return field("current_user",String.class);
}
return function("current_user",SQLDataType.VARCHAR);
}
