{
  try {
    if (rs != null) {
      rs.close();
      rs=null;
      for (      Record record : this) {
        if (record instanceof AbstractRecord) {
          ((AbstractRecord)record).rs=null;
          ((AbstractRecord)record).rsIndex=0;
        }
      }
    }
  }
 catch (  SQLException e) {
    throw translate("Cannot close ResultSet",e);
  }
}
