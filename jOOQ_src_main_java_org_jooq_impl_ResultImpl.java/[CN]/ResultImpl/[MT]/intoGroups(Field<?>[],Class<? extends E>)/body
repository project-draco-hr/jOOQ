{
  if (keys == null) {
    keys=new Field[0];
  }
  Map<Record,List<E>> map=new LinkedHashMap<Record,List<E>>();
  FieldList keyList=new FieldList(keys);
  for (  R record : this) {
    @SuppressWarnings({"rawtypes","unchecked"}) Record key=new RecordImpl(keyList);
    for (    Field<?> field : keys) {
      Utils.setValue(key,field,record,field);
    }
    List<E> list=map.get(key);
    if (list == null) {
      list=new ArrayList<E>();
      map.put(key,list);
    }
    list.add(record.into(type));
  }
  return map;
}
