{
  Map<K,List<V>> map=new LinkedHashMap<K,List<V>>();
  for (  R record : this) {
    K k=record.getValue(key);
    V v=record.getValue(value);
    List<V> result=map.get(k);
    if (result == null) {
      result=new ArrayList<V>();
      map.put(k,result);
    }
    result.add(v);
  }
  return map;
}
