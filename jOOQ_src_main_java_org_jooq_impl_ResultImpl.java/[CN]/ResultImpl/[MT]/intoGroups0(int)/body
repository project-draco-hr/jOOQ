{
  Map<K,Result<R>> map=new LinkedHashMap<K,Result<R>>();
  for (  R record : this) {
    K val=(K)record.getValue(keyFieldIndex);
    Result<R> result=map.get(val);
    if (result == null) {
      result=new ResultImpl<R>(configuration,fields);
      map.put(val,result);
    }
    result.add(record);
  }
  return map;
}
