{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  String name : create().select(Sequences.SEQUENCE_NAME).from(SEQUENCES).where(Sequences.SEQUENCE_SCHEMA.equal(getSchemaName())).orderBy(Sequences.SEQUENCE_NAME).fetch(Sequences.SEQUENCE_NAME)) {
    result.add(new DefaultSequenceDefinition(this,name));
  }
  return result;
}
