{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  String name : create().select(Sequences.SEQUENCE_NAME).from(SEQUENCES).where(Sequences.SEQUENCE_SCHEMA.equal(getInputSchema())).orderBy(Sequences.SEQUENCE_NAME).fetch(Sequences.SEQUENCE_NAME)) {
    DefaultDataTypeDefinition type=new DefaultDataTypeDefinition(this,H2DataType.BIGINT.getTypeName(),0,0);
    result.add(new DefaultSequenceDefinition(getSchema(),name,type));
  }
  return result;
}
