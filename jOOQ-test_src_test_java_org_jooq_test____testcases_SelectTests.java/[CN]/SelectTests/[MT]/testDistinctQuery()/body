{
  Result<Record1<Integer>> result=create().selectDistinct(TBook_AUTHOR_ID()).from(TBook()).orderBy(TBook_AUTHOR_ID()).fetch();
  assertEquals(2,result.size());
  assertEquals(Integer.valueOf(1),result.get(0).getValue(TBook_AUTHOR_ID()));
  assertEquals(Integer.valueOf(2),result.get(1).getValue(TBook_AUTHOR_ID()));
  assertEquals(2,create().selectDistinct(TBook_AUTHOR_ID()).from(TBook()).fetch().size());
}
