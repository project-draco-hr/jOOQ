{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  String separator="";
  for (  Object o : value) {
    sb.append(separator);
    if (o == null) {
      sb.append(o);
    }
 else {
      sb.append("\"");
      sb.append(o.toString().replaceAll("\"","\"\""));
      sb.append("\"");
    }
    separator=", ";
  }
  sb.append("}");
  return sb.toString();
}
