{
  return create().selectFrom(org.jooq.test.adaptiveserver.generatedclasses.tables.XUnused.X_UNUSED).where(org.jooq.test.adaptiveserver.generatedclasses.tables.XUnused.ID_REF.equal(getValue(org.jooq.test.adaptiveserver.generatedclasses.tables.XUnused.ID))).and(org.jooq.test.adaptiveserver.generatedclasses.tables.XUnused.NAME_REF.equal(getValue(org.jooq.test.adaptiveserver.generatedclasses.tables.XUnused.NAME))).fetch();
}
