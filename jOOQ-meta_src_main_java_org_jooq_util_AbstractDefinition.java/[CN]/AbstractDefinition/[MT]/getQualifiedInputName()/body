{
  StringBuilder sb=new StringBuilder();
  String separator="";
  for (  Definition part : getDefinitionPath()) {
    if (part instanceof SchemaDefinition && ((SchemaDefinition)part).isDefaultSchema()) {
      continue;
    }
    sb.append(separator);
    sb.append(part.getInputName());
    separator=".";
  }
  return sb.toString();
}
