{
  StringBuilder sb=new StringBuilder();
  sb.append("QueryLog[prepare=");
  sb.append(StopWatch.format(prepareTime));
  sb.append(", bind=");
  sb.append(StopWatch.format(bindTime));
  sb.append(", execute=");
  sb.append(StopWatch.format(executeTime));
  sb.append(", query=[");
  sb.append(query);
  sb.append("]]");
  return sb.toString();
}
