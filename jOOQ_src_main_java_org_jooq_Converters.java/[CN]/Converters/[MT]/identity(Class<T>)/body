{
  return new Converter<T,T>(){
    /** 
 * Generated UID
 */
    private static final long serialVersionUID=-8331976721627671263L;
    @Override public final T from(    T t){
      return t;
    }
    @Override public final T to(    T t){
      return t;
    }
    @Override public final Class<T> fromType(){
      return type;
    }
    @Override public final Class<T> toType(){
      return type;
    }
  }
;
}
