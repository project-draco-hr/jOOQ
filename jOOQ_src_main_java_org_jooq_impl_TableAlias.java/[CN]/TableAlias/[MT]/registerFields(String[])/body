{
  Row row=this.alias.wrapped().fieldsRow();
  int size=row.getDegree();
  for (int i=0; i < size; i++) {
    Field<?> field=row.field(i);
    String name=field.getName();
    if (fieldAliases != null && fieldAliases.length > i) {
      name=fieldAliases[i];
    }
    registerTableField(field,name);
  }
}
