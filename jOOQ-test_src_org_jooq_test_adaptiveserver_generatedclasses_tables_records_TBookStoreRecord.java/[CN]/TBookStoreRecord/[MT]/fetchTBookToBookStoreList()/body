{
  return create().selectFrom(org.jooq.test.adaptiveserver.generatedclasses.tables.TBookToBookStore.T_BOOK_TO_BOOK_STORE).where(org.jooq.test.adaptiveserver.generatedclasses.tables.TBookToBookStore.BOOK_STORE_NAME.equal(getValue(org.jooq.test.adaptiveserver.generatedclasses.tables.TBookStore.NAME))).fetch();
}
