{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  Record record : create().select(Iisequences.SEQ_NAME.trim(),Iisequences.DATA_TYPE.trim()).from(IISEQUENCES).where(Iisequences.SEQ_OWNER.equal(getSchemaName())).orderBy(Iisequences.SEQ_NAME).fetch()) {
    DataTypeDefinition type=new DefaultDataTypeDefinition(this,record.getValue(Iisequences.DATA_TYPE.trim()),0,0);
    result.add(new DefaultSequenceDefinition(getSchema(),record.getValue(Iisequences.SEQ_NAME.trim()),type));
  }
  return result;
}
