  assertEquals("\"cnt\"",ref().render(count1a));
  assertEquals("count(*) \"cnt\"",decI().render(count1a));
  assertEquals("count(*) \"cnt\"",dec().render(count1a));
  assertEquals(1,internal(count1a).bindReference(create,statement));
