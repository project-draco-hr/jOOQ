  assertEquals("sum(\"TABLE1\".\"ID1\")",r_decI().render(sum1));
  assertEquals("sum(\"TABLE1\".\"ID1\")",r_dec().render(sum1));
  assertEquals(1,b_ref().bind(sum1).peekIndex());
  Field<BigDecimal> sum2=FIELD_ID1.sum().as("value");
