{
  if (inlineParameters) {
    StringBuilder sb=new StringBuilder();
    sb.append(array.getName());
    sb.append("(");
    String separator="";
    for (    Object object : array.get()) {
      sb.append(separator);
      sb.append(internal(create(configuration).val(object)).toSQLReference(configuration,inlineParameters));
      separator=", ";
    }
    sb.append(")");
    return sb.toString();
  }
 else {
    return "?";
  }
}
