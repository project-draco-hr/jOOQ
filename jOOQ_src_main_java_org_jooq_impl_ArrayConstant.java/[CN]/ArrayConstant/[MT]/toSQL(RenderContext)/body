{
  if (context.inline()) {
    context.literal(array.getName());
    context.sql("(");
    String separator="";
    for (    Object object : array.get()) {
      context.sql(separator);
      context.sql(create(context).val(object));
      separator=", ";
    }
    context.sql(")");
  }
 else {
    context.sql("?");
  }
}
