{
  for (  Method m : startCount.keySet()) {
    Integer starts=startCount.get(m);
    Integer ends=endCount.get(m);
    if (!StringUtils.equals(starts,ends)) {
      unbalanced++;
      logger.info("Unbalanced",String.format("(open, close): (%1$3s, %2$3s) at %3$s",starts,ends == null ? 0 : ends,m.toString().replace("public void ","").replaceAll("( throws.*)?","")));
    }
  }
  return unbalanced;
}
