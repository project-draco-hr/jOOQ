{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.person.tables.Address.ADDRESS).where(org.jooq.examples.sqlserver.adventureworks.person.tables.Address.ADDRESS.ADDRESSID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesorderheader.SALESORDERHEADER.BILLTOADDRESSID))).fetchOne();
}
