{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currencyrate.CURRENCYRATE).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currencyrate.CURRENCYRATE.CURRENCYRATEID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesorderheader.SALESORDERHEADER.CURRENCYRATEID))).fetchOne();
