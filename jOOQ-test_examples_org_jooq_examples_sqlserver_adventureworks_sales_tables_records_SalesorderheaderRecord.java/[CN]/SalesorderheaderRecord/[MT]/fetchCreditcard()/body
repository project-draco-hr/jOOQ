{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Creditcard.CREDITCARD).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Creditcard.CREDITCARD.CREDITCARDID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Salesorderheader.SALESORDERHEADER.CREDITCARDID))).fetchOne();
}
