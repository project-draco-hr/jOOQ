{
  return create().select(concat(Keycoluse.TABNAME,val("__"),Keycoluse.CONSTNAME).as("constraint_name"),Keycoluse.TABSCHEMA.trim(),Keycoluse.TABNAME,Keycoluse.COLNAME).from(KEYCOLUSE).join(TABCONST).on(Keycoluse.TABSCHEMA.equal(Tabconst.TABSCHEMA)).and(Keycoluse.TABNAME.equal(Tabconst.TABNAME)).and(Keycoluse.CONSTNAME.equal(Tabconst.CONSTNAME)).where(Keycoluse.TABSCHEMA.in(getInputSchemata())).and(Tabconst.TYPE.equal(constraintType)).orderBy(Keycoluse.TABSCHEMA.asc(),Keycoluse.TABNAME.asc(),Keycoluse.CONSTNAME.asc(),Keycoluse.COLSEQ.asc()).fetch();
}
