{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  String name : create().select(Sequences.SEQNAME).from(SEQUENCES).where(Sequences.SEQSCHEMA.equal(getSchemaName())).orderBy(Sequences.SEQNAME).fetch(Sequences.SEQNAME)) {
    result.add(new DefaultSequenceDefinition(this,name));
  }
  return result;
}
