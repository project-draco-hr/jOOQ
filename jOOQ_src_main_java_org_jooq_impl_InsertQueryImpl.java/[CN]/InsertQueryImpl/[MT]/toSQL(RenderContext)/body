{
  if (onDuplicateKeyUpdate) {
switch (context.configuration().getDialect()) {
case CUBRID:
case MYSQL:
{
        toSQLInsert(context);
        context.formatSeparator().keyword("on duplicate key update ").sql(updateMap);
        break;
      }
case H2:
{
      throw new SQLDialectNotSupportedException("The ON DUPLICATE KEY UPDATE clause cannot be simulated for " + context.configuration().getDialect());
    }
case DB2:
case HSQLDB:
case ORACLE:
case SQLSERVER:
case SYBASE:
{
    context.sql(toMerge(context.configuration()));
    break;
  }
default :
throw new SQLDialectNotSupportedException("The ON DUPLICATE KEY UPDATE clause cannot be simulated for " + context.configuration().getDialect());
}
}
 else if (onDuplicateKeyIgnore) {
switch (context.configuration().getDialect()) {
case MYSQL:
{
toSQLInsert(context);
break;
}
case CUBRID:
{
FieldMapForUpdate update=new FieldMapForUpdate();
Field<?> field=getInto().field(0);
update.put(field,field);
toSQLInsert(context);
context.formatSeparator().keyword("on duplicate key update ").sql(update);
break;
}
case H2:
{
throw new SQLDialectNotSupportedException("The ON DUPLICATE KEY IGNORE clause cannot be simulated for " + context.configuration().getDialect());
}
case DB2:
case HSQLDB:
case ORACLE:
case SQLSERVER:
case SYBASE:
{
context.sql(toMerge(context.configuration()));
break;
}
default :
throw new SQLDialectNotSupportedException("The ON DUPLICATE KEY IGNORE clause cannot be simulated for " + context.configuration().getDialect());
}
}
 else {
toSQLInsert(context);
}
}
