{
  BatchBindStep step=factory.batch(factory.insertInto(T_AUTHOR,T_AUTHOR.ID,T_AUTHOR.FIRST_NAME,T_AUTHOR.LAST_NAME).values(param("id"),param("first"),param("last")));
  for (  Author author : authors) {
    Integer id=factory.nextval(S_AUTHOR_ID).intValue();
    step=step.bind(id,author.getFirstName(),author.getLastName());
  }
  step.execute();
}
