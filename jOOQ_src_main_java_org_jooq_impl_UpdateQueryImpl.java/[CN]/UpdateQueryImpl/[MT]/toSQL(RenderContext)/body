{
  context.keyword("update ").declareTables(true).sql(getInto()).declareTables(false).formatSeparator().keyword("set ").formatIndentLockStart().sql(updateMap).formatIndentLockEnd();
  if (!(getWhere() instanceof TrueCondition)) {
    context.formatSeparator().keyword("where ").sql(getWhere());
  }
}
