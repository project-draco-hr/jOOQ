{
  context.bind(getInto());
  if (multiRow != null) {
    context.bind(multiRow);
    if (multiValue != null) {
      context.bind(multiValue);
    }
 else {
      context.subquery(true).bind(multiSelect).subquery(false);
    }
  }
 else {
    context.bind(updateMap);
  }
  context.bind(condition);
}
