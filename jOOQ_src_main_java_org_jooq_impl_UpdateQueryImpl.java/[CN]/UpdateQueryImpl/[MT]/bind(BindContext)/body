{
  boolean declareTables=context.declareTables();
  context.declareTables(true).visit(getInto()).declareTables(declareTables);
  if (multiRow != null) {
    context.visit(multiRow);
    if (multiValue != null) {
      context.visit(multiValue);
    }
 else {
      context.subquery(true).visit(multiSelect).subquery(false);
    }
  }
 else {
    context.visit(updateMap);
  }
  context.visit(condition);
  bindReturning(context);
}
