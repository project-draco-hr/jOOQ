{
  context.declareFields(true).bind((QueryPart)getSelect0()).declareFields(false).declareTables(true).bind((QueryPart)getFrom()).declareTables(false).bind(getWhere()).bind(getConnectBy()).bind(getConnectByStartWith()).bind((QueryPart)getGroupBy()).bind(getHaving()).bind((QueryPart)getOrderBy());
