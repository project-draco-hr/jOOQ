{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currencyrate.CURRENCYRATE).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currencyrate.CURRENCYRATE.FROMCURRENCYCODE.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Currency.CURRENCY.CURRENCYCODE))).fetch();
}
