{
  List<Blob> blobs=BLOBS.get();
  List<Clob> clobs=CLOBS.get();
  if (blobs != null) {
    for (    Blob blob : blobs) {
      JDBCUtils.safeFree(blob);
    }
    BLOBS.remove();
  }
  if (clobs != null) {
    for (    Clob clob : clobs) {
      JDBCUtils.safeFree(clob);
    }
    CLOBS.remove();
  }
  LOCAL_CONFIGURATION.remove();
  LOCAL_CONNECTION.remove();
}
