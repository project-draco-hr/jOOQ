{
  return sqlDataType == SQLDataType.BINARY || sqlDataType == SQLDataType.BIT || sqlDataType == SQLDataType.CHAR || sqlDataType == SQLDataType.LONGNVARCHAR || sqlDataType == SQLDataType.LONGVARBINARY || sqlDataType == SQLDataType.LONGVARCHAR || sqlDataType == SQLDataType.NCHAR || sqlDataType == SQLDataType.NVARCHAR || sqlDataType == SQLDataType.VARBINARY || sqlDataType == SQLDataType.VARCHAR;
}
