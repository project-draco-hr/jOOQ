{
  return create().selectFrom(org.jooq.test.cubrid.generatedclasses.tables.XUnused.X_UNUSED).where(org.jooq.test.cubrid.generatedclasses.tables.XUnused.NAME_REF.equal(getValue(org.jooq.test.cubrid.generatedclasses.tables.XUnused.ID,String.class))).and(org.jooq.test.cubrid.generatedclasses.tables.XUnused.ID_REF.equal(getValue(org.jooq.test.cubrid.generatedclasses.tables.XUnused.NAME,Integer.class))).fetch();
}
