{
switch (configuration.getDialect()) {
case ASE:
case SQLSERVER:
case SYBASE:
{
      return Factory.concat(field,Factory.repeat(character,length.sub(Factory.length(field))));
    }
case SQLITE:
{
    return Factory.field("{0} || replace(replace(substr(quote(zeroblob(({1} + 1) / 2)), 3, ({2} - length({3}))), '''', ''), '0', {4})",String.class,field,length,length,field,character);
  }
default :
{
  return function("rpad",SQLDataType.VARCHAR,field,length,character);
}
}
}
