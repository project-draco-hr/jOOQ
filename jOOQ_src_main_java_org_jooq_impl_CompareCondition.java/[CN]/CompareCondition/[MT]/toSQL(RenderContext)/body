{
  context.sql(field1).sql(" ");
  if (field2.isNullLiteral()) {
switch (comparator) {
case EQUALS:
      context.sql("is null");
    return;
case NOT_EQUALS:
  context.sql("is not null");
return;
}
}
boolean cast=context.getDialect() == DB2 && field2 instanceof Concat;
context.sql(comparator.toSQL()).sql(" ").sql(cast ? "cast(" : "").sql(field2).sql(cast ? " as varchar(4000))" : "");
if (escape != null) {
context.sql(" escape '").sql(escape).sql("'");
}
}
