{
  context.sql(field1).sql(" ");
  if (field2.isNullLiteral()) {
switch (comparator) {
case EQUALS:
      context.sql("is null");
    return;
case NOT_EQUALS:
  context.sql("is not null");
return;
}
}
context.sql(comparator.toSQL()).sql(" ").sql(field2);
if (escape != null) {
context.sql(" escape '").sql(escape).sql("'");
}
}
