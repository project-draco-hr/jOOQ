{
  if (log.isDebugEnabled()) {
    if (ctx.query() != null) {
      log.debug("Executing query",ctx.sql());
      String inlined=ctx.query().getSQL(true);
      if (!ctx.sql().equals(inlined)) {
        log.debug("-> with bind values",inlined);
      }
    }
 else     if (!StringUtils.isBlank(ctx.sql())) {
      log.debug("Executing query",ctx.sql());
    }
  }
}
