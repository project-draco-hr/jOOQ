{
  StringBuilder result=new StringBuilder();
  result.append(StringUtils.toCamelCase(definition.getOutputName()));
  if (mode == Mode.RECORD) {
    result.append("Record");
  }
 else   if (mode == Mode.DAO) {
    result.append("Dao");
  }
 else   if (mode == Mode.INTERFACE) {
    result.insert(0,"I");
  }
  return result.toString();
}
