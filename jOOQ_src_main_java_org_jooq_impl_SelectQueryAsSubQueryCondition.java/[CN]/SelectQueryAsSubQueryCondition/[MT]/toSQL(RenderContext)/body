{
  if (context.subquery()) {
    context.sql(field).sql(" ").keyword(comparator.toSQL()).sql(" (").formatIndentStart().formatNewLine().sql(query).formatIndentEnd().formatNewLine().sql(")");
  }
 else {
    context.sql(field).sql(" ").keyword(comparator.toSQL()).sql(" (").subquery(true).formatIndentStart().formatNewLine().sql(query).formatIndentEnd().formatNewLine().subquery(false).sql(")");
  }
}
