{
  try {
    Field field=type().getField(name);
    field.set(object,unwrap(value));
    return this;
  }
 catch (  Exception e1) {
    try {
      accessible(type().getDeclaredField(name)).set(object,unwrap(value));
      return this;
    }
 catch (    Exception e2) {
      throw new ReflectException(e2);
    }
  }
}
