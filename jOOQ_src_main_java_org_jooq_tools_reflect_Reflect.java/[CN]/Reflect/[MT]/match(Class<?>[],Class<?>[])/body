{
  if (declaredTypes.length == actualTypes.length) {
    for (int i=0; i < actualTypes.length; i++) {
      if (actualTypes[i] == NULL.class)       continue;
      if (wrapper(declaredTypes[i]).isAssignableFrom(wrapper(actualTypes[i])))       continue;
      return false;
    }
    return true;
  }
 else {
    return false;
  }
}
