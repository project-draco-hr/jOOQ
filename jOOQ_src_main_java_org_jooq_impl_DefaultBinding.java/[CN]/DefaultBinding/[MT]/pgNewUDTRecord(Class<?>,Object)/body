{
  if (object == null) {
    return null;
  }
  return Utils.newRecord(true,(Class<UDTRecord<?>>)type).operate(new RecordOperation<UDTRecord<?>,SQLException>(){
    @Override public UDTRecord<?> operate(    UDTRecord<?> record) throws SQLException {
      List<String> values=PostgresUtils.toPGObject(object.toString());
      Row row=record.fieldsRow();
      for (int i=0; i < row.size(); i++) {
        pgSetValue(record,row.field(i),values.get(i));
      }
      return record;
    }
  }
);
}
