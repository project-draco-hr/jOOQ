{
  try {
    try {
      return Long.valueOf(date);
    }
 catch (    NumberFormatException e) {
      if (pattern.contains(".") && !date.contains("."))       return new SimpleDateFormat(pattern).parse(date + ".0").getTime();
 else       return new SimpleDateFormat(pattern).parse(date).getTime();
    }
  }
 catch (  ParseException e) {
    throw new SQLException("Could not parse date " + date,e);
  }
}
