{
  if (context.configuration().dialect() == SQLDialect.DERBY) {
    if (field.getDataType().isNumeric() && BOOLEAN.equals(getSQLDataType())) {
      context.bind(asDecodeNumberToBoolean());
      return;
    }
 else     if (field.getDataType().isString() && BOOLEAN.equals(getSQLDataType())) {
      context.bind(asDecodeVarcharToBoolean());
      return;
    }
  }
  context.bind(field);
}
