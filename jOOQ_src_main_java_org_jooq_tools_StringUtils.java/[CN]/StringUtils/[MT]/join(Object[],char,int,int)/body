{
  if (array == null) {
    return null;
  }
  int noOfItems=(endIndex - startIndex);
  if (noOfItems <= 0) {
    return EMPTY;
  }
  StringBuilder buf=new StringBuilder(noOfItems * 16);
  for (int i=startIndex; i < endIndex; i++) {
    if (i > startIndex) {
      buf.append(separator);
    }
    if (array[i] != null) {
      buf.append(array[i]);
    }
  }
  return buf.toString();
}
