{
  context.declareFields(true).visit(getSelect0()).declareFields(false).declareTables(true).visit(getFrom()).declareTables(false).visit(getWhere()).visit(getConnectByStartWith()).visit(getConnectBy()).visit(getGroupBy()).visit(getHaving()).visit(getOrderBy());
