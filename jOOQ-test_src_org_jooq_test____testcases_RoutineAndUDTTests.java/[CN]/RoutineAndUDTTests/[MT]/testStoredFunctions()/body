  assertEquals(null,create().selectOne().where(f4b.equal(1)).and(f1b.equal(1)).fetchOne());
  Result<Record3<Integer,Integer,Integer>> result2=create().select(FNumberField(1).cast(Integer.class),FNumberField(TAuthor_ID()).cast(Integer.class),FNumberField(FNumberField(TAuthor_ID())).cast(Integer.class)).from(TAuthor()).orderBy(TAuthor_ID()).fetch();
