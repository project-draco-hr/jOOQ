  Field<Integer> f1b=FAuthorExistsField("Paulo").cast(Integer.class);
  Field<Integer> f2b=FAuthorExistsField("Shakespeare").cast(Integer.class);
  Field<Integer> f3b=FOneField().cast(Integer.class);
