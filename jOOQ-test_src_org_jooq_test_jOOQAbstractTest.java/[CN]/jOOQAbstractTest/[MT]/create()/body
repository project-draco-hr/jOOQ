{
  String defaultSchema=System.getProperty("org.jooq.settings.defaultSchema","");
  Boolean renderSchema=Boolean.valueOf(System.getProperty("org.jooq.settings.renderSchema","true"));
  Settings settings=SettingsTools.defaultSettings().withExecuteDebugging(ExecuteDebugging.SERVER).withExecuteDebuggingPort(5555).withRenderSchema(renderSchema).withRenderMapping(new RenderMapping().withDefaultSchema(defaultSchema)).withExecuteListeners(TestStatisticsListener.class.getName());
  return create(settings);
}
