{
  String defaultSchema=System.getProperty("org.jooq.settings.defaultSchema","");
  Boolean renderSchema=Boolean.valueOf(System.getProperty("org.jooq.settings.renderSchema","true"));
  Settings settings=SettingsTools.defaultSettings().withRenderSchema(renderSchema).withRenderMapping(new RenderMapping().withDefaultSchema(defaultSchema)).withExecuteLogging(false).withExecuteListeners(TestStatisticsListener.class.getName(),PrettyPrinter.class.getName());
  return create(settings);
}
