{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Shoppingcartitem.SHOPPINGCARTITEM).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Shoppingcartitem.SHOPPINGCARTITEM.PRODUCTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Product.PRODUCT.PRODUCTID))).fetch();
}
