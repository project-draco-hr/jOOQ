{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Productmodel.PRODUCTMODEL).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Productmodel.PRODUCTMODEL.PRODUCTMODELID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Product.PRODUCT.PRODUCTMODELID))).fetchOne();
}
