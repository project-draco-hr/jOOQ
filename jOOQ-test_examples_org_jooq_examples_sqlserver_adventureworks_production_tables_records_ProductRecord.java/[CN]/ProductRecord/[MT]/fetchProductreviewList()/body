{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Productreview.PRODUCTREVIEW).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Productreview.PRODUCTREVIEW.PRODUCTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.production.tables.Product.PRODUCT.PRODUCTID))).fetch();
}
