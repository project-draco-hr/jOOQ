{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.sales.tables.Store.STORE).where(org.jooq.examples.sqlserver.adventureworks.sales.tables.Store.STORE.CUSTOMERID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.sales.tables.Storecontact.STORECONTACT.CUSTOMERID))).fetchOne();
}
