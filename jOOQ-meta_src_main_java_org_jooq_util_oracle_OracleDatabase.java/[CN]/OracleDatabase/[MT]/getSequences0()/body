{
  List<SequenceDefinition> result=new ArrayList<SequenceDefinition>();
  for (  String name : create().select(SEQUENCE_NAME).from(ALL_SEQUENCES).where(SEQUENCE_OWNER.equal(getSchemaName())).orderBy(SEQUENCE_NAME).fetch(SEQUENCE_NAME)) {
    result.add(new DefaultSequenceDefinition(this,name));
  }
  return result;
}
