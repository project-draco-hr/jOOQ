{
  return create().select(ALL_CONS_COLUMNS.OWNER,ALL_CONS_COLUMNS.CONSTRAINT_NAME,ALL_CONS_COLUMNS.TABLE_NAME,ALL_CONS_COLUMNS.COLUMN_NAME).from(ALL_CONS_COLUMNS.join(ALL_CONSTRAINTS).on(ALL_CONS_COLUMNS.OWNER.equal(ALL_CONSTRAINTS.OWNER)).and(ALL_CONS_COLUMNS.CONSTRAINT_NAME.equal(ALL_CONSTRAINTS.CONSTRAINT_NAME))).where(ALL_CONSTRAINTS.CONSTRAINT_TYPE.equal(constraintType).and(ALL_CONSTRAINTS.CONSTRAINT_NAME.notLike("BIN$%")).and(ALL_CONS_COLUMNS.OWNER.upper().in(getInputSchemata()))).orderBy(ALL_CONS_COLUMNS.OWNER,ALL_CONS_COLUMNS.CONSTRAINT_NAME,ALL_CONS_COLUMNS.POSITION).fetch();
}
