{
  for (  Record record : create().select(ALL_CONSTRAINTS.CONSTRAINT_NAME,ALL_CONSTRAINTS.TABLE_NAME,ALL_CONS_COLUMNS.COLUMN_NAME,ALL_CONSTRAINTS.R_CONSTRAINT_NAME).from(ALL_CONSTRAINTS).join(ALL_CONS_COLUMNS).on(ALL_CONSTRAINTS.OWNER.equal(ALL_CONS_COLUMNS.OWNER)).and(ALL_CONSTRAINTS.TABLE_NAME.equal(ALL_CONS_COLUMNS.TABLE_NAME)).and(ALL_CONSTRAINTS.CONSTRAINT_NAME.equal(ALL_CONS_COLUMNS.CONSTRAINT_NAME)).where(ALL_CONSTRAINTS.CONSTRAINT_TYPE.equal("R")).and(ALL_CONSTRAINTS.OWNER.equal(getInputSchema())).orderBy(ALL_CONSTRAINTS.TABLE_NAME,ALL_CONSTRAINTS.CONSTRAINT_NAME,ALL_CONS_COLUMNS.POSITION).fetch()) {
    String foreignKeyName=record.getValue(ALL_CONSTRAINTS.CONSTRAINT_NAME);
    String foreignKeyTableName=record.getValue(ALL_CONSTRAINTS.TABLE_NAME);
    String foreignKeyColumnName=record.getValue(ALL_CONS_COLUMNS.COLUMN_NAME);
    String uniqueKeyName=record.getValue(ALL_CONSTRAINTS.R_CONSTRAINT_NAME);
    TableDefinition referencingTable=getTable(foreignKeyTableName);
    if (referencingTable != null) {
      ColumnDefinition column=referencingTable.getColumn(foreignKeyColumnName);
      relations.addForeignKey(foreignKeyName,uniqueKeyName,column);
    }
  }
}
