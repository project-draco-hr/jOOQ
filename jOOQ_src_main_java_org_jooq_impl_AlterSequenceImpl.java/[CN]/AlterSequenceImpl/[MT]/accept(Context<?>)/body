{
  ctx.start(ALTER_SEQUENCE_SEQUENCE).keyword("alter sequence").sql(" ").visit(sequence).end(ALTER_SEQUENCE_SEQUENCE).start(ALTER_SEQUENCE_RESTART);
  T with=restartWith;
  if (with == null) {
    if (ctx.family() == INFORMIX)     ctx.sql(" ").keyword("restart with 1");
 else     ctx.sql(" ").keyword("restart");
  }
 else {
    ctx.sql(" ").keyword("restart with").sql(" ").sql(with.toString());
  }
  ctx.end(ALTER_SEQUENCE_RESTART);
}
