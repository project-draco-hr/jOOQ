{
  UniqueKey<?> key=getPrimaryKey();
  Value<T> val=getValue0(field);
  if (key == null || !key.getFields().contains(field)) {
    val.setValue(value);
  }
 else   if (val.isChanged()) {
    val.setValue(value);
  }
 else {
    val.setValue(value,true);
    if (val.isChanged()) {
      changed(true);
    }
  }
}
