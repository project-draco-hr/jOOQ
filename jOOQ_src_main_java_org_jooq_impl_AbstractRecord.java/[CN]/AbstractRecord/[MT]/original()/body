{
  return Utils.newRecord(fetched,(Class<AbstractRecord>)getClass(),fields.fields.fields,configuration()).operate(new RecordOperation<AbstractRecord,RuntimeException>(){
    @Override public AbstractRecord operate(    AbstractRecord record) throws RuntimeException {
      for (int i=0; i < originals.length; i++) {
        record.values[i]=originals[i];
        record.originals[i]=originals[i];
      }
      return record;
    }
  }
);
}
