{
switch (dialect().family()) {
case ASE:
case CUBRID:
case DB2:
case DERBY:
case FIREBIRD:
case H2:
case INGRES:
case MARIADB:
case MYSQL:
case ORACLE:
case SQLITE:
case SQLSERVER:
case SYBASE:
    log.info("SKIPPING","RESTART IDENTITY tests");
  return;
}
jOOQAbstractTest.reset=false;
InsertResultStep<I> insert=create().insertInto(TIdentity(),TIdentity_VAL()).values(1).returning(TIdentity_ID());
int id1=insert.fetchOne().getValue(TIdentity_ID());
create().truncate(TIdentity()).continueIdentity().execute();
int id2=insert.fetchOne().getValue(TIdentity_ID());
assertEquals(id1 + 1,id2);
create().truncate(TIdentity()).restartIdentity().execute();
int id3=insert.fetchOne().getValue(TIdentity_ID());
assertEquals(1,id3);
}
