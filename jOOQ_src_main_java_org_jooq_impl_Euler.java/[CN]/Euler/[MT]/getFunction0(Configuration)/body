{
switch (configuration.getDialect()) {
case ASE:
case CUBRID:
case DB2:
case DERBY:
case H2:
case HSQLDB:
case INGRES:
case MYSQL:
case ORACLE:
case POSTGRES:
case SQLSERVER:
case SYBASE:
    return Factory.exp(one());
case SQLITE:
  return literal(Math.E,BigDecimal.class);
default :
return function("e",getDataType());
}
}
