{
switch (configuration.dialect().family()) {
case ASE:
case CUBRID:
case DB2:
case DERBY:
case FIREBIRD:
case H2:
case HSQLDB:
case INGRES:
case MARIADB:
case MYSQL:
case ORACLE:
case POSTGRES:
case SQLSERVER:
case SYBASE:
    return DSL.exp(one());
case SQLITE:
  return inline(Math.E,BigDecimal.class);
default :
return function("e",getDataType());
}
}
