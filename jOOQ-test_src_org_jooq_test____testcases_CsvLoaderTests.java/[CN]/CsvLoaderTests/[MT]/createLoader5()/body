{
  Loader<A> loader;
  String csv="1,\"Kafka\"\n" + "2,Frisch";
  loader=create().loadInto(TAuthor()).onDuplicateKeyIgnore().onErrorAbort().loadCSV(csv).fields(TAuthor_ID(),TAuthor_LAST_NAME()).ignoreRows(0).execute();
  System.out.println("Loader 5 " + create().selectFrom(TAuthor()).fetch().formatJSON());
  return loader;
}
