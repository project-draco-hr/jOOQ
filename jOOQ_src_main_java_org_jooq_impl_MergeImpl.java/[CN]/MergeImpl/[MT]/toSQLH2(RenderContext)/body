{
  context.keyword("merge into").sql(" ").declareTables(true).visit(table).formatSeparator();
  context.sql("(");
  Utils.fieldNames(context,getH2Fields());
  context.sql(")");
  if (!getH2Keys().isEmpty()) {
    context.sql(" ").keyword("key").sql(" (");
    Utils.fieldNames(context,getH2Keys());
    context.sql(")");
  }
  if (h2Select != null) {
    context.sql(" ").visit(h2Select);
  }
 else {
    context.sql(" ").keyword("values").sql(" (").visit(getH2Values()).sql(")");
  }
}
