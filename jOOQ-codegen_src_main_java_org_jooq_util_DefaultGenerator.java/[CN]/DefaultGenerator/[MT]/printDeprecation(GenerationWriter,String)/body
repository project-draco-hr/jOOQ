{
  if (deprecation != null) {
    out.println("\t *");
    String[] strings=deprecation.split("[\n\r]+");
    for (int i=0; i < strings.length; i++) {
      if (i == 0) {
        out.println("\t * @deprecated " + strings[i]);
      }
 else {
        out.println("\t *             " + strings[i]);
      }
    }
  }
}
