{
  out.print("\tpublic void ");
  out.print(strategy.getJavaSetterName(element,Mode.DEFAULT));
  out.print("(");
  out.print(getJavaType(element.getType()));
  out.print(" value)");
  if (printBody) {
    out.println(" {");
    out.println("\t\tsetValue(" + strategy.getFullJavaIdentifier(element) + ", value);");
    out.println("\t}");
  }
 else {
    out.println(";");
  }
}
