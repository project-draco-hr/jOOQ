{
  out.println();
  out.println("\t/**");
  out.println("\t * The type " + udt.getQualifiedOutputName());
  out.println("\t */");
  out.print("\tpublic static ");
  out.print(strategy.getFullJavaClassName(udt));
  out.print(" ");
