{
switch (configuration.getDialect()) {
case CUBRID:
case DB2:
case FIREBIRD:
case ORACLE:
case SQLSERVER:
    return Factory.decode().when(condition,inline(true)).otherwise(inline(false));
case DERBY:
case H2:
case HSQLDB:
case MYSQL:
case POSTGRES:
case SQLITE:
case ASE:
case INGRES:
case SYBASE:
  return condition;
}
return condition;
}
