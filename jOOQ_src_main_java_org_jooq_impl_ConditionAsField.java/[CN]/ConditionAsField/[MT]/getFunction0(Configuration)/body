{
switch (configuration.dialect().family()) {
case DB2:
case ORACLE:
case SQLSERVER:
case SYBASE:
case CUBRID:
case FIREBIRD:
    return DSL.decode().when(condition,inline(true)).otherwise(inline(false));
case DERBY:
case H2:
case HSQLDB:
case MARIADB:
case MYSQL:
case POSTGRES:
case SQLITE:
case ASE:
case INGRES:
  return condition;
}
return condition;
}
