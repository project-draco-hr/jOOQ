{
  try {
    rs.absolute(row);
    rs.deleteRow();
    Object[][] newRowData=new Object[rowData.length - 1][];
    System.arraycopy(rowData,0,newRowData,0,row);
    System.arraycopy(rowData,row + 1,newRowData,row,newRowData.length - row - 1);
    rowData=newRowData;
    return true;
  }
 catch (  SQLException ex) {
    ex.printStackTrace();
  }
  return false;
}
