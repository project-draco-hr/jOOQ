{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Department.DEPARTMENT).where(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Department.DEPARTMENT.DEPARTMENTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Employeedepartmenthistory.EMPLOYEEDEPARTMENTHISTORY.DEPARTMENTID))).fetchOne();
}
