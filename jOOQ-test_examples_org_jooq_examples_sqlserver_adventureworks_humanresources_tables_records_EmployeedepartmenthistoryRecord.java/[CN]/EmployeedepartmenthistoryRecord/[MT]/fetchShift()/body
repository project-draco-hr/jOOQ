{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Shift.SHIFT).where(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Shift.SHIFT.SHIFTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.humanresources.tables.Employeedepartmenthistory.EMPLOYEEDEPARTMENTHISTORY.SHIFTID))).fetchOne();
}
