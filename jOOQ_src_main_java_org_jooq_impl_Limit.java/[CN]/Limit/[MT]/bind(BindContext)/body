{
switch (context.configuration().dialect()) {
case DERBY:
case SQLSERVER:
case SQLSERVER2012:
{
      context.bind(offsetOrZero);
      context.bind(numberOfRows);
      break;
    }
case MARIADB:
case MYSQL:
case HSQLDB:
case H2:
case POSTGRES:
case SQLITE:
{
    context.bind(numberOfRows);
    context.bind(offsetOrZero);
    break;
  }
case CUBRID:
{
  context.bind(offsetOrZero);
  context.bind(numberOfRows);
  break;
}
case FIREBIRD:
{
context.bind(getLowerRownum());
context.bind(getUpperRownum());
break;
}
case ASE:
case INGRES:
{
break;
}
case SYBASE:
{
if (!rendersParams) {
}
 else {
context.bind(getLowerRownum());
context.bind(getUpperRownum());
}
break;
}
case DB2:
case SQLSERVER2008:
{
if (offset == null && !rendersParams) {
}
 else {
context.bind(getLowerRownum());
context.bind(getUpperRownum());
}
break;
}
case ORACLE:
case ORACLE10G:
case ORACLE11G:
case ORACLE12C:
{
context.bind(getUpperRownum());
context.bind(getLowerRownum());
break;
}
default :
{
context.bind(numberOfRows);
context.bind(offsetOrZero);
break;
}
}
}
