{
switch (context.configuration().dialect()) {
case DERBY:
{
      context.visit(offsetOrZero);
      context.visit(numberOfRows);
      break;
    }
case MARIADB:
case MYSQL:
case HSQLDB:
case H2:
case POSTGRES:
case SQLITE:
{
    context.visit(numberOfRows);
    context.visit(offsetOrZero);
    break;
  }
case CUBRID:
{
  context.visit(offsetOrZero);
  context.visit(numberOfRows);
  break;
}
case FIREBIRD:
{
context.visit(getLowerRownum());
context.visit(getUpperRownum());
break;
}
default :
{
context.visit(numberOfRows);
context.visit(offsetOrZero);
break;
}
}
}
