{
  if (dialect() == SQLDialect.SQLITE) {
    log.info("SKIPPING","System functions test");
    return;
  }
  Field<?> user=trim(lower(currentUser()));
  Record record=create().select(user).fetchOne();
  assertTrue(Arrays.asList("test","lukas","sa","root@localhost","postgres","dbo","dba","dba@lukas-hp","dba@lukas-omni27").contains(record.getValue(user)));
}
