{
  boolean extraParentheses=asList(ORACLE).contains(context.getDialect());
  boolean subquery=context.subquery();
  context.sql(RowImpl.this).sql(" ").keyword(operator.toSQL()).sql(" (").sql(extraParentheses ? "(" : "").subquery(true).sql(other).subquery(subquery).sql(extraParentheses ? ")" : "").sql(")");
}
