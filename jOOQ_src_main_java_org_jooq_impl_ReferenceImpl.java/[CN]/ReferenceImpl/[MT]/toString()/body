{
  StringBuilder sb=new StringBuilder();
  sb.append("FOREIGN KEY (");
  String s1="";
  for (  Field<?> field : getFields()) {
    sb.append(s1);
    sb.append(field);
    s1=", ";
  }
  sb.append(") REFERENCES ");
  sb.append(key.getTable());
  sb.append("(");
  String s2="";
  for (  Field<?> field : getFields()) {
    sb.append(s2);
    sb.append(field);
    s2=", ";
  }
  sb.append(")");
  return sb.toString();
}
