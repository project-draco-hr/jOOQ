{
  if (id == null) {
    this.id=UUID.randomUUID();
  }
 else {
    this.id=id;
  }
  this.hitCount=hitCount;
  if (hitCount != null) {
    currentHitCount=new AtomicInteger(hitCount);
  }
  this.matcher=matcher;
  this.isBreaking=isBreaking;
  this.before=before;
  this.replace=replace;
  this.after=after;
}
