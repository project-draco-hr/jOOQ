{
  if (hitCount != null && currentHitCount.get() <= 0) {
    return false;
  }
  boolean hasMatcher=false;
  if (matcher != null) {
    if (!matcher.matches(queryInfo)) {
      return false;
    }
    hasMatcher=true;
  }
  if (hitCount != null) {
    int currentHitCount_=currentHitCount.decrementAndGet();
    if (currentHitCount_ > 0) {
      return false;
    }
    if (currentHitCount_ < 0) {
      currentHitCount.set(0);
      return false;
    }
    hasMatcher=true;
  }
  return hasMatcher;
}
