{
  PreparedStatement stmt=null;
  if (bookIds == null)   bookIds=new Object[0];
  String sql=create(connection).select().from(T_BOOK).where(TBook.ID.in(INTEGER.getArrayDataType().convert(bookIds))).orderBy(TBook.ID.asc()).getSQL();
  stmt=connection.prepareStatement(sql);
  int i=1;
  for (  Object o : bookIds) {
    stmt.setObject(i++,o);
  }
  return stmt.executeQuery();
}
