{
  return create().selectFrom(org.jooq.examples.sqlserver.adventureworks.production.tables.Product.PRODUCT).where(org.jooq.examples.sqlserver.adventureworks.production.tables.Product.PRODUCT.PRODUCTID.equal(getValue(org.jooq.examples.sqlserver.adventureworks.purchasing.tables.Purchaseorderdetail.PURCHASEORDERDETAIL.PRODUCTID))).fetchOne();
}
