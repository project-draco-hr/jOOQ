{
switch (dialect().family()) {
case ASE:
case DB2:
case INGRES:
case SQLSERVER:
case DERBY:
case FIREBIRD:
case SQLITE:
    log.info("SKIPPING","REGEX tests");
  return;
}
Result<B> result=create().selectFrom(TBook()).where(TBook_CONTENT_TEXT().likeRegex(".*conscious.*")).or(TBook_TITLE().notLikeRegex(".*m.*")).orderBy(TBook_ID()).fetch();
assertEquals(2,result.size());
assertEquals(1,(int)result.get(0).getValue(TBook_ID()));
assertEquals(4,(int)result.get(1).getValue(TBook_ID()));
}
