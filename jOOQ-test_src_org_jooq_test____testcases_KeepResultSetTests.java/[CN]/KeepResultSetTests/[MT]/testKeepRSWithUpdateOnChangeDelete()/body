{
switch (dialect()) {
case SQLITE:
    log.info("SKIPPING","KeepResultSet tests");
  return;
}
jOOQAbstractTest.reset=false;
DSLContext create=create(new NoStatementAfterCRUDListener(Delete.class,"delete"));
Result<B> books=create.selectFrom(TBook()).orderBy(TBook_ID()).keepResultSet(UPDATE_ON_CHANGE).fetch();
B b2=books.get(1);
assertEquals(1,b2.delete());
ResultSet rs=b2.resultSet();
rs.beforeFirst();
assertTrue(rs.next());
assertTrue(rs.next());
assertTrue(rs.next());
assertFalse(rs.next());
System.out.println(books);
}
